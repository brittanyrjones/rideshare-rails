<%= form_for @trip, url: passenger_trips_path(@trip.passenger_id) do |f| %>

<%= f.label :driver_id, class: "trip" %></br>
<%= f.text_field :driver_id %></br>

<%= f.label :passenger_id %></br>
<%= f.text_field :passenger_id %></br>

<%= f.label :date %></br>
<%= f.date_field :date %></br>

<%= f.label :rating %></br>
<%= f.text_field :rating %></br>


<%= f.submit action_name, method: :put %>



<% end %>
